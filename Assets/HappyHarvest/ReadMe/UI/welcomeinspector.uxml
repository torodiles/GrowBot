<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <engine:Label name="intro" enable-rich-text="true" text="intro text here" style="color: rgb(255, 255, 255); font-size: 13px; white-space: pre-wrap; -unity-text-align: middle-left; margin-top: 8px; margin-right: 8px; margin-bottom: 8px; margin-left: 8px;">
        <Bindings>
            <engine:DataBinding property="text" data-source-path="introText" binding-mode="ToTarget" />
        </Bindings>
    </engine:Label>
    <engine:Label name="resources" enable-rich-text="false" text="RESOURCES" style="color: rgb(255, 255, 255); font-size: 13px; white-space: pre-wrap; -unity-text-align: middle-left; margin-top: 8px; margin-right: 8px; margin-bottom: 8px; margin-left: 8px; -unity-font-style: bold;" />
    <engine:VisualElement name="root-resources" style="flex-grow: 1; flex-direction: row; flex-wrap: wrap; background-color: rgb(0, 0, 0); padding-bottom: 6px; border-top-left-radius: 14px; border-top-right-radius: 14px; border-bottom-right-radius: 14px; border-bottom-left-radius: 14px;" />
</engine:UXML>
